<table border = "1">
		<thead>
			<tr>
				<th>#</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Email</th>
			</tr>
		</thead>

		<tbody>
			<% @users.each_with_index do |user,index|%>
			<tr>
				<td> <%= index + 1 %></td>
				<td> <%= user.first_name %></td>
				<td> <%= user.last_name %> </td>
				<td> <%= user.email %> </td>
			</tr>
			<% end %>
		</tbody>
	</table>